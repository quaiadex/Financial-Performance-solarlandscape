	partition dim_calendar = calculated
		mode: import
		source =
				
				VAR _today = [#TODAY]
				RETURN
				ADDCOLUMNS(
				    CALENDARAUTO(),
				    "Year", FORMAT([Date], "YYYY"),
				    "Year Current", IF(FORMAT([#TODAY], "YYYY") = FORMAT([Date], "YYYY"), "Current Year",  FORMAT([Date], "YYYY") ),
				    "Semester", "H" & IF( MONTH([Date]) <= 6, 1, 2 ),
				    "Quarter", "Q" & QUARTER([Date]),
				    "Month Num", MONTH([Date]),
				    "Month Name MMM", FORMAT( [Date], "MMM"),
				    "YearMonth YYYY/MMM", FORMAT( [Date], "MMM/YY"),
				    "YearMonth Current Month YYYY/MMM",  IF(FORMAT([#TODAY], "YYYYMM") = FORMAT([Date], "YYYYMM"), "Current Month",  FORMAT( [Date], "MMM/YY")),
				    "YearMonth YYYYMM", FORMAT( [Date], "YYYYMM")
				)

	annotation PBI_Id = 3c61898681b84aeeabceb665ee42fb52

